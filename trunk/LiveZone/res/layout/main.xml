<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/LinearLayout01" 
android:layout_width="fill_parent" 
android:layout_height="fill_parent" 
xmlns:android="http://schemas.android.com/apk/res/android" android:isScrollContainer="true" android:orientation="vertical">

<ImageButton android:id="@+id/ImageButton01" android:layout_width="wrap_content" android:layout_height="wrap_content"></ImageButton>


<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/asdf" android:text="Available providers 1"></TextView>
<EditText android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/AvailableProviders"></EditText>
<TextView android:id="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Provider in use"></TextView>
<EditText android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/Provider"></EditText>
<TextView android:id="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Latitude"></TextView>
<EditText android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/Latitude"></EditText>



<TextView android:text="Longtitude" android:id="@+id/TextView03" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
<EditText android:text="@+id/EditText01" android:id="@+id/Longtitude" android:layout_width="fill_parent" android:layout_height="wrap_content"></EditText>
<TextView android:id="@+id/TextView02" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Accuracy"></TextView>
<EditText android:text="@+id/EditText01" android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/Accuracy"></EditText>
<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Set Proximity Alert" android:id="@+id/ProximityAlert"></Button>


<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/StopProximity" android:text="StopProximity"></Button>



</LinearLayout>
